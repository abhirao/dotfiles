[user]
	email = abhirao@gmail.com
	name = Abhi Rao
[color]
	ui = true
        diff = auto
        status = auto
        branch = auto
        interactive = auto
[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    dc = diff --cached
    amend = commit --amend
    aa = add --all
    head = !git l -1
    h = !git head
    r = !git --no-pager l -20
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    l = log --graph --abbrev-commit --date=relative 
    la = !git l --all
    div = divergence
    gn = goodness
    gnc = goodness --cached
    fa = fetch --all
    pom = push origin master
    b = branch
    unstage = reset HEAD
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    remotes = remote -v
[gc]
    auto=1
[merge]
    summary=true
